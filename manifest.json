{
  "manifest_version": 3,
  "name": "Suspend Tab",
  "version": "1.0",
  "description": "Suspends a tab when clicked",
  "permissions": ["tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Suspend Tab",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
